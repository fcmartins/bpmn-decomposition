include "bag.tyc"

let
  -- a bag 
  bag = NewBag []
in
io ! prints [bag ! get []] |
io ! printi [bag ! size []] |
bag ! put ["Hello "] |
bag ! put ["TyCO "] |
io ! printi [bag ! size []]